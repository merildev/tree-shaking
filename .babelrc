{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": [
          "ie >= 11",
          "> 1%",
          "last 3 Chrome versions",
          "last 2 Edge versions",
          "last 2 Firefox versions"
        ]
      },
      "useBuiltIns": "usage"
    }],
    "@babel/react",
  ],
  "env": {
    "development": {
      "presets": [
        "@babel/env",
        "@babel/react"
      ]
    },
    "production": {
      "presets": [
        "@babel/env",
        "@babel/react"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-namespace-from"
  ]
}
